# Clerk Keys (Get from your Clerk Dashboard)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Clerk Frontend API URL for Convex Auth (Next.js requires NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_CLERK_FRONTEND_API_URL=

# Convex URL (Get from the Convex Dashboard after first deploy: npx convex deploy)
NEXT_PUBLIC_CONVEX_URL=

# OpenRouter Key (Get from your OpenRouter Dashboard)
OPENROUTER_API_KEY=


# Deployment used by `npx convex dev`
CONVEX_DEPLOYMENT=

# Upstash Redis URL (Get from your Upstash Dashboard or connect from Vercel)
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=
REDIS_URL=